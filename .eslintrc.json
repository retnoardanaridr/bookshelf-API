{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "key-spacing": "off",
        "eol-last": "off",
        "indent": "off",
        "no-multiple-empty-lines": "off",
        "no-unused-vars": "off",
        "max-len": "off",
        "object-curly-newline": "off",
        "consistent-return": "off"
    }
}
